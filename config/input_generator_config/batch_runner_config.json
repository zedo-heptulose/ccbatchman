{
  "program" : "batch_runner",
  "version" : "0.1",
  "pre_submit_lines" : [
	  "conda init bash",
	  "source ~/.bashrc",
	  "conda activate ccbatchman",
	  "export PYTHONUNBUFFERED=1"
  ],
  "post_submit_lines" : null,
  "num_cores" : 1,
  "mem_per_cpu_GB" : 3,
  "runtime" : "3-00:00:00",
  "write_directory" : ".",
  "job_basename" : "batch_runner",
  "verbosity" : false,
  "input_file" : "batchfile.csv",
  "ledger_filename" : "__ledger__.csv",
  "max_jobs" : 1,
  "restart_failed_jobs" : false
}
